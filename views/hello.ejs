<h1>Hello World</h1>

<ul class="table-body">
  <% users.forEach((user) => { %>
    <li>
      <div class="item-data">
        <span class="id-column"><%= user.id %></span>
        <span class="name-column"><%= user.name %></span>
      </div>
    </li>
  <% }) %>
</ul>

